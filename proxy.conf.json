{
  "/api/*": {
    "target": "http://localhost:8080",
    "changeOrigin": false,
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api": "/api"
    }
  },
  "/ws": {
    "target": "http://localhost:8080",
    "ws": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api": ""
    }
  }
}